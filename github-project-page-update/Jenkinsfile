#!groovy

def workerNode = "devel8"

pipeline {
    agent {
        docker {
            label workerNode
            image "docker.dbc.dk/python3-deploy-env"
            alwaysPull true
        }
    }
    stages {
        stage("update confluence") {
            steps {
              sh """pip install git+https://github.com/DBCDK/pytools.git
                    github-projects-page 464519179 --verbose --topics ait"""
            }
        }
    }
}
